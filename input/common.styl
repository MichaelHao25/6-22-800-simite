        span
            display inline-block
            vertical-align middle
        &.active
            span
                background rgba(0,0,0,0)
                &:after
                    top 0
                    transform rotateZ(45deg)
                    transition top 0.3s,bottom 0.3s,transform 0.3s 0.3s
                &:before
                    bottom 0
                    transform rotateZ(-45deg)
                    transition top 0.3s,bottom 0.3s,transform 0.3s 0.3s
        span
            position relative
            width 100%
            height 2px
            background #00a0e9
            cursor pointer
            transition 0.3s
            &:before
            &:after
                content ''
                position absolute
                left 0
                right 0
                height 2px
                width 100%
                background #00a0e9
                transition 0.3s
            &:after
                top -20px
                transition top 0.3s 0.3s,bottom 0.3s 0.3s,transform 0.3s
                @media(max-width 767px)
                    top -13px
            &:before
                bottom -20px
                transition top 0.3s 0.3s,bottom 0.3s 0.3s,transform 0.3s
                @media(max-width 767px)
                    bottom -13px
                
        i
